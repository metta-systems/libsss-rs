use super::frame_type::FrameType;

/*
```
ofs : sz : description
  0 :  1 : Frame type (6 - PRIORITY)
  1 :  4 : Stream Local ID (LSID) in sender ID space
  5 :  4 : Priority value
```
*/
#[packet]
pub struct PriorityFrame {
    #[construct_with(u8)]
    pub frame_type: FrameType,
    #[payload]
    pub payload: Vec<u8>,
}
